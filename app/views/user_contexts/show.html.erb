<div class="page-header">
  <%= link_to user_contexts_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_user_context_path(@user_context), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show user_context</h1>
</div>

<dl class="dl-horizontal">
  <dt>Value:</dt>
  <dd><%= @user_context.value %></dd>

  <dt>Context:</dt>
  <dd><%= @user_context.context %></dd>

  <dt>User:</dt>
  <dd><%= @user_context.user %></dd>

</dl>
