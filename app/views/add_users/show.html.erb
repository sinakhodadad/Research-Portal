<div class="page-header">
  <%= link_to add_users_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_add_user_path(@add_user), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show users</h1>
</div>

<dl class="dl-horizontal">
  <dt>Email:</dt>
  <dd><%= @add_user.email %></dd>

  <dt>Name:</dt>
  <dd><%= @add_user.name %></dd>

  <dt>Family:</dt>
  <dd><%= @add_user.family %></dd>

  <dt>Role:</dt>
  <dd><%= @add_user.role %></dd>

</dl>
